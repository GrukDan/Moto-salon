<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXPasswordField?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="559.0" prefWidth="879.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" >
   <children>
      <ImageView fitHeight="616.0" fitWidth="886.0" layoutX="-6.0" layoutY="-54.0">
         <image>
            <Image url="@../img/biker.jpg" />
         </image>
      </ImageView>
      <JFXTextField fx:id="user_name_fd" layoutX="14.0" layoutY="47.0" prefHeight="45.0" prefWidth="286.0" promptText="Имя">
         <font>
            <Font size="19.0" />
         </font></JFXTextField>
      <JFXTextField fx:id="user_surname_fd" layoutX="14.0" layoutY="136.0" prefHeight="45.0" prefWidth="286.0" promptText="Фамилия">
         <font>
            <Font size="19.0" />
         </font></JFXTextField>
      <JFXTextField fx:id="user_email_fd" layoutX="14.0" layoutY="232.0" prefHeight="45.0" prefWidth="286.0" promptText="Email">
         <font>
            <Font size="19.0" />
         </font></JFXTextField>
      <JFXPasswordField fx:id="user_password_fd" layoutX="14.0" layoutY="317.0" prefHeight="45.0" prefWidth="286.0" promptText="Пароль">
         <font>
            <Font size="19.0" />
         </font></JFXPasswordField>
      <Button fx:id="save_btn" layoutX="71.0" layoutY="405.0" mnemonicParsing="false" onAction="#save" prefHeight="55.0" prefWidth="170.0" style="-fx-background-color: lightgreen; -fx-background-radius: 25; -fx-border-radius: 25; -fx-border-color: black;" text="Сохранить">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Button>
      <Button fx:id="close_btn" layoutX="742.0" layoutY="513.0" mnemonicParsing="false" onAction="#close" prefHeight="35.0" prefWidth="112.0" text="Закрыть">
         <font>
            <Font name="System Bold" size="17.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
